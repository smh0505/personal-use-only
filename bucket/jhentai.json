{
    "version": "8.0.2+258",
    "description": "A cross-platform manga app made for e-hentai & exhentai by Flutter",
    "homepage": "https://github.com/jiangtian616/JHenTai",
    "license": "Apache-2.0",
    "notes": [
        "Config and data are stored in %USERPROFILE%\\AppData\\Roaming\\top.jtmonster\\jhentai",
        "They will not be automatically removed during uninstallation even with -p flag. Delete them manually if needed."
    ],
    "url": "https://github.com/jiangtian616/JHenTai/releases/download/v8.0.2/JHenTai_8.0.2%2B258_Windows.zip",
    "hash": "d0b4440d946e00a41389c06f67edeccfcda92564bfe8b8be93cdde116974e997",
    "extract_dir": "JHenTai_8.0.2+258",
    "shortcuts": [
        [
            "jhentai.exe",
            "JHenTai"
        ]
    ],
    "bin": [
        [
            "jhentai.exe",
            "jhentai"
        ]
    ],
    "checkver": {
        "script": "(Invoke-RestMethod -Uri 'https://api.github.com/repos/jiangtian616/JHenTai/releases/latest').assets | Where-Object name -like \"*Windows.zip\" | ForEach-Object { \"$($_.name)&$($_.browser_download_url)\" }",
        "regex": "JHenTai_(?<Version>\\d+(\\.\\d+)*(\\+\\d+)?)_Windows\\.zip&(?<URL>.*)",
        "replace": "${Version}"
    },
    "autoupdate": {
        "url": "$matchURL",
        "extract_dir": "JHenTai_$matchVersion",
        "hash": {
            "mode": "download"
        }
    }
}
